				;--------------------------------------
				getString MACRO bufferaddr, buffertyp, bufsize
				; Gets an input string to return the input in buffer.
				; Avoid passing arguments in edx, ecx, eax.
				;--------------------------------------
					push	edx
					push	ecx
					push	eax

					mov		edx, bufferaddr
					mov		ecx, buffertyp
					call	ReadString

					mov		[bufsize], eax

					pop		eax
					pop		ecx
					pop		edx

				ENDM

